{
  "name": "Yahoo Fantasy Football Redirect",
  "description": "Always redirects Yahoo to Yahoo Fantasy Sports.",
  "version": "1.0",
  "manifest_version": 2,
  "background": {"scripts":["background.js"]},
  "permissions": [
    "webRequest",
    "*://yahoo.com/*",
    "*://www.yahoo.com/*",
    "webRequestBlocking"
  ],
  "icons": { "16": "icon.png",
             "48": "icon.png",
             "128": "icon.png" }
}